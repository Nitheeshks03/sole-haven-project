{
  "type": "module",
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "all": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node seeder.js",
    "data:destroy": "node seeder.js -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend -- --cwd frontend"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
